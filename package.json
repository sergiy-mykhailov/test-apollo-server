{
  "name": "test-apollo-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "create": "node_modules/.bin/sequelize db:create",
    "drop": "node_modules/.bin/sequelize db:drop",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "start": "cross-env DEBUG=server nodemon ./scripts/start.js",
    "server1": "cross-env DEBUG=server nodemon ./test-server.js",
    "build": "node ./scripts/build.js",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js"
  },
  "dependencies": {
    "apollo-server-express": "^2.1.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "morgan": "^1.7.0",
    "mysql2": "^1.6.1",
    "node-env-file": "^0.1.8",
    "node-wit": "^5.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^4.1.1",
    "serve-favicon": "~2.5.0"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.3.0"
  }
}
